{
	"name": "@camtv/video-player",
	"version": "1.0.0",
	"description": "Video player for hls, youtube, and video files",
	"keywords": [
		"video",
		"player",
		"hls",
		"youtube"
	],
	"author": "Stefanone_91 <stefano.camtv@gmail.com>",
	"homepage": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/camtv/video-player"
	},
	"main": "dist/video-player.js",
	"scripts": {
		"purge": "rimraf dist demo",
		"test": "echo \"Error: run tests from root\" && exit 1",
		"dev": "webpack-dev-server --config webpack/config.babel.js",
		"build:demo": "cross-env NODE_ENV=production webpack --config webpack/demo.babel.js",
		"build:dist": "cross-env NODE_ENV=production webpack --config webpack/dist.babel.js",
		"build:standalone": "cross-env NODE_ENV=production webpack --config webpack/standalone.babel.js",
		"build": "npm run purge && npm run build:dist && npm run build:standalone && npm run build:demo",
		"preversion": "npm run lint && npm run test",
		"version": "auto-changelog -p && npm run build && git add CHANGELOG.md dist",
		"postpublish": "npm run clean"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.9.0",
		"@babel/register": "^7.9.0",
		"auto-changelog": "^2.0.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"cross-env": "^7.0.2",
		"css-loader": "^3.5.2",
		"eslint": "^6.8.0",
		"eslint-loader": "^4.0.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.17.3",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.12.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"svg-url-loader": "^5.0.0",
		"terser-webpack-plugin": "^2.3.5",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"dependencies": {
		"video.js": "^7.8.1",
		"videojs-contrib-quality-levels": "^2.0.9"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	}
}
