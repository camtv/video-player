{
	"name": "@camtv/video-player",
	"version": "1.0.1",
	"description": "Video player for hls, youtube, and video files",
	"keywords": [
		"video",
		"player",
		"hls",
		"youtube"
	],
	"author": "Stefanone_91 <stefano.camtv@gmail.com>",
	"homepage": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/camtv/video-player"
	},
	"main": "./dist/video-player.js",
	"scripts": {
		"purge": "rimraf dist",
		"test": "echo \"Error: run tests from root\" && exit 1",
		"dev:demo": "webpack-dev-server --config webpack/demo.babel.js",
		"dev:module": "webpack --config webpack/module.babel.js",
		"dev:standalone": "webpack --config webpack/standalone.babel.js",
		"dev": "npm run dev:demo & npm run dev:module",
		"build:demo": "cross-env NODE_ENV=production webpack --mode production --config webpack/demo.babel.js",
		"build:module": "cross-env NODE_ENV=production webpack --mode production --config webpack/module.babel.js",
		"build:standalone": "cross-env NODE_ENV=production webpack --mode production --config webpack/standalone.babel.js",
		"build": "npm run purge && npm-run-all -p build:*",
		"version": "auto-changelog -p && git add CHANGELOG.md",
		"prepublishOnly": "npm run version",
		"publish": "node ./build/gh-release.js",
		"postpublish": "npm run clean"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/register": "^7.9.0",
		"@mdi/js": "^5.1.45",
		"auto-changelog": "^2.0.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"bestzip": "^2.1.5",
		"cross-env": "^7.0.2",
		"css-loader": "^3.5.2",
		"eslint": "^6.8.0",
		"eslint-loader": "^4.0.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.20.2",
		"gh-release": "^3.5.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.5",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-loader": "^3.0.0",
		"prompt": "^1.0.0",
		"remark-parse": "^8.0.2",
		"remark-stringify": "^8.0.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.4",
		"svg-url-loader": "^5.0.0",
		"terser-webpack-plugin": "^2.3.5",
		"unified": "^9.0.0",
		"webpack": "^4.42.1",
		"webpack-bundle-analyzer": "^3.7.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"dependencies": {
		"video.js": "^7.7.5",
		"videojs-contrib-quality-levels": "^2.0.9"
	}
